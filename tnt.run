mxram 10240 ;
nstates 32 ;
nstates NOGAPS ;

piwe=12;

procedure file.tnt ;
log tnt.log ;
taxname= ;

hold 100000 ;
xpiwe(*; mult=replic 100 tbr hold 1000;
xpiwe&; piwe&;
export= trees.tre ;

bbreak=tbr ;
majority * ;

ttags= ;
resample boot replications 1000;
ttags & bt.svg thickness 7 italics fontsize 15;
export < bt.tre ;

length * ;
stats.run;

quit ;
